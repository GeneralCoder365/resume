{
    "ignoreCommand": "git diff --quiet HEAD^ HEAD -- . ':!pdfs/' ':!.github/' ':!build/' ':!README.md'",
    "rewrites": [
        { "source": "/resume", "destination": "/resume/index.html" },
        { "source": "/resume/", "destination": "/resume/index.html" },
        { "source": "/resume/static/:path*", "destination": "/static/:path*" },
        { "source": "/resume/manifest.json", "destination": "/manifest.json" },
        { "source": "/resume/*", "destination": "/resume/index.html" }
    ]
}